    "portfolio",
    "website"
  ],
  "author": "John Smith <john@smith.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DWNWND/joker/issues"
  },
  "homepage": "https://github.com/DWNWND/joker#readme",
  "devDependencies": {
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.2.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
